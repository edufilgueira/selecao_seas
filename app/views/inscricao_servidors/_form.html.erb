$(document).on("input", "#inscricao_servidor_recurso_candidato", function () {
  var limite = 500;
  var caracteresDigitados = $(this).val().length;
  var caracteresRestantes = limite - caracteresDigitados;
  $(".caracteres_recurso_dados").text(caracteresRestantes + " restantes");

  if(caracteresRestantes == 0){
    texto = $(this).val().substring(0,$(this).val().length - 1);
    $(this).val(texto);
  } else if(caracteresRestantes < 0){
    texto = $(this).val().substring(0,500);
    $(this).val(texto);
    $(".caracteres_recurso_dados").text("0 restantes");
  }
});


$(document).ready(function () {
  var id = $("#inscricao_servidor_cargo_id").val();
  criterio_de_cargo(id);
  
  if($('#inscricao_servidor_termo_aceito').is(':checked'))
    $('#botao_salvar_inscricao_servidor').prop("disabled", false);
  else
    $('#botao_salvar_inscricao_servidor').prop("disabled", true);

  if($('#inscricao_servidor_deficiente').is(':checked'))
    $('#inscricao_servidor_deficiente_fl').css("display", "block");
  else
    $('#inscricao_servidor_deficiente_fl').css("display", "none");
  
  $("#inscricao_servidor_telefone_celular").keyup(function(){
    var id = $(this).val();
    if(id.length < 15){
      $("#valid_telefone_celular").css('display', 'block');
    } else
      $("#valid_telefone_celular").css('display', 'none');
  });
  $("#inscricao_servidor_telefone_celular").blur(function(){
    var id = $(this).val();
    if(id.length < 15){
      $("#valid_telefone_celular").css('display', 'block');
      $("#inscricao_servidor_telefone_celular").focus();
    } else
      $("#valid_telefone_celular").css('display', 'none');
  });

  //DESLIGA OS CAMPOS DO FORM E LIBERA A DINAMICA DO BOTÃO EDITAR SALVAR
  <% if(!form.object.new_record?) %>
    <% if(!form.object.finalizar? ) %>
      desabilitar_campos_form(true);
      $('#botao_salvar_inscricao_servidor').prop("disabled", true);
      $('#botao_editar_inscricao_servidor').prop("disabled", true);
      
      $("#botao_editar_inscricao_servidor").click(function(e){
        desabilitar_campos_form(false);
        $('#finalizar_inscricao').css('display','none');
      /**
       * CHAMA FUNÇÃO criterio_de_cargo(id) PARA ORGANIZAR OS BOTÕES DE ensino_medio e ensino_superior, 
       * POIS O SCRIPT EXIGE O FOCO AO SALVAR, PORISSO É NECESSÁRIO DESABILITAR QUEM PRECISA SER DESABILITADO
       **/
        criterio_de_cargo(id);
        $('html, body').animate({scrollTop:240}, 'slow');
        e.preventDefault(); 
      });
    <% end %>
  <% end %>
  validar_pontuacao_sem_anexo();
});

function desabilitar_campos_form(cmd){
  $('.form-control,input,select,textarea,a').each(function(){
    $(this).prop("disabled", cmd);
  });
  $('table a').each(function(){
    if(cmd)
      $(this).css("pointer-events", "none");
    else
      $(this).css("pointer-events", "visible");
  });
}


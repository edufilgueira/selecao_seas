<%= form_with(model: inscricao_servidor, local: true, html: { multipart: true }) do |form| %>
  <% configuracao = Configuracao.where(status: true).last %>
  <% if Time.now.to_date < configuracao.data_inicio_selecao %>
    <%= render partial: 'abertura' %>
  <% elsif Time.now.to_date >= configuracao.data_termino_selecao.to_date %>
    <%= render partial: 'encerramento' %>
  <% end %>

  <div id="alerta_anexo_sem_pontuacao" role="alert" class="alert alert-danger" style="display:none">
    <ul style="color: #990000;">
      <li>Pontuação Obrigatória! Informe a pontuação relacionada ao arquivo anexado</li>
    </ul>
  </div>
  <div id="alerta_idade" role="alert" class="alert alert-danger" style="display:none">
    <ul style="color: #990000;">
      <li>Não é permitido candidato com idade inferior a 18 anos</li>
    </ul>
  </div>

  <% unless params[:mensagem_erro].blank? %>
    <br>
    <div class="alert alert-danger" role="alert">
      <% params[:mensagem_erro].each do |mensagem| %>
      <ul style="color: #990000;">
        <li><%= mensagem %></li>
      </ul>
      <% end %>
    </div>
  <% end %>
  <br>

  <% if inscricao_servidor.errors.any? %>
    <div id="error_explanation">
      <!--<h2><%#= pluralize(inscricao_servidor.errors.count, "error") %> prohibited this inscricao_servidor from being saved:</h2>-->
      <div class="alert alert-danger" role="alert">
        <ul style="color: #990000;">
        <% inscricao_servidor.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
        </ul>
      </div>
    </div>
  <% end %>

  <% if(params[:id].nil?) %>
    <div style="width: 100%;text-align: center;"><%= image_tag('progresso_inicio.jpg') %></div><br>
  <% elsif(!params[:id].nil? and !form.object.finalizar? ) %>
    <div style="width: 100%;text-align: center;"><%= image_tag('progresso_conferencia.jpg') %></div><br>
  <% elsif(!params[:id].nil? and form.object.finalizar? ) %>
    <div style="width: 100%;text-align: center;">
      <%= image_tag('progresso_final.jpg') %>
      <div style="height: 35px;width: 144px;display: inline-block;padding-top: 15px;background-image: url(/assets/tracos.jpg);margin-left: -24px;"></div>
      <a href="#" onClick="window.print()" class="link"> <%= image_tag('print.png',style:"width: 52px;margin-top: -32px;", title:"Clique aqui para imprimir sua página de inscrição")  %></a>
      
    </div><br>
  <% end %>

  <% if(!params[:id].nil?) %>
    <% inscricao_servidor = InscricaoServidor.find_by(slug: params[:id]) %>
    <% if(inscricao_servidor.finalizar?) %>
      <div style="border: 1px solid #cdcdcd; padding: 5px 10px 0px 10px; text-align: center; background-color: #48bb6b; color: #FFF; font-weight: bold; margin: 20px 29% 20px 29%; border-radius: 10px;">
        <div>Nº Inscrição</div>
        <div style="font-size: 40px;margin: -10px 0 0 0;"><%= "%.5i" %inscricao_servidor.id %></div>
      </div>
    <% end %>
  <% end %>
  <% if(!inscricao_servidor.finalizar?) %>
    <div id="error_explanation">
      <div class="alert alert-info" role="alert" style="center;color:#2b2b2b;background-color:#fff7db;border-color: #f2dd88;">
        <h1><i class="fas fa-exclamation-triangle"  style="color: #2b2b2b;"></i> Atenção</h1>
        <p>Prezado candidato, somente finalize sua inscrição, clicando no botão de cor verde ("Finalizar Inscrição"), localizado no final deste formulário, após realizar a conferência dos seus dados.</p>
      </div>
    </div><br>
  <% end %>

  <% if(!form.object.finalizar? ) %>
    <h2>Dados Pessoais</h2>
    <div class="row">
      <div class="col-md-12">
        <%= form.label :cargo_id, class: 'required' %>
        <%= form.collection_select :cargo_id, Cargo.where(tipo_cargo:"Servidor"), :id, :descricao, {include_blank: '--selecione--'}, class: 'form-control', required: true %>
      </div>
	  </div>
    <br>
    <div class="row">
      <div class="col-md-3">
        <%= form.label :cpf, class: 'required' %>
        <% if (params[:cpf].nil?)%>
          <%= form.text_field :cpf, class: 'form-control mask-cpf', required: true %>
        <% else %>
          <%= form.text_field :cpf, value: params[:cpf], class: 'form-control mask-cpf', required: true %>
        <% end %>
        <span id="val_cpf" class="requerido"></span>
      </div>
      <div class="col-md-9">
        <%= form.label :nome_completo, class: 'required' %>
        <%= form.text_field :nome_completo, class: 'form-control', required: true %>
      </div>
    </div>
    <br>
    <div class="row">
      <div class="col-md-6">
        <%= form.label :naturalidade, class: 'required' %>
        <%= form.text_field :naturalidade, class: 'form-control', required: true %>
      </div>
      <div class="col-md-6">
        <%= form.label :nacionalidade, class: 'required' %>
        <%= form.text_field :nacionalidade, class: 'form-control', required: true %>
      </div>
    </div>
    <br>
    <div class="row">
      <div class="col-md-8">
        <%= form.label :nome_mae, class: 'required' %>
        <%= form.text_field :nome_mae, class: 'form-control', required: true %>
      </div>
      <div class="col-md-4">
        <%= form.label :data_nascimento, class: 'required' %>
        <% if(form.object.new_record?) %>
          <%= form.text_field :data_nascimento, class: 'form-control mask-date', required: true, onblur: "verificar_data_nascimento(this)" %>
        <% else %>
          <%= form.text_field :data_nascimento, value: form&.object&.data_nascimento&.strftime('%d/%m/%Y'), class: 'form-control mask-date', required: true, onblur: "verificar_data_nascimento(this)" %>
        <% end %>
        <label id="validador_data_nascimento" style="color: #990000;font: normal 12px arial;display: none;margin-top: 5px;"></label>
      </div>
    </div>
    <br>
    <div class="row">
      <div class="col-md-6">
        <%= form.label :sexo, class: 'required' %>
        <%= form.select :sexo, ["Feminino", "Masculino"], {prompt: 'selecione'}, class: 'form-control', required: true %>
      </div>
      <div class="col-md-6">
        <%= form.label :estado_civil, class: 'required' %>
        <%= form.select :estado_civil, ["Solteiro(a)", "Casado(a)", "Separado(a)", "Divorciado(a)", 'Viúvo(a)', "União Estável", "Outros"], {prompt: 'selecione'}, class: 'form-control', required: true %>
      </div>
    </div>
    <br>
    <div class="row">
      <div class="col-md-6">
        <%= form.label :rg, class: 'required' %>
        <%= form.text_field :rg, class: 'form-control', type: :number, required: true %>
      </div>
      <div class="col-md-6">
        <%= form.label :orgao_emissor, class: 'required' %>
        <%= form.text_field :orgao_emissor, class: 'form-control', required: true %>
      </div>
    </div>
    <br>
    <div class="row">
      <div class="col-md-12">
        <%= form.label :endereco, class: 'required' %>
        <%= form.text_field :endereco, class: 'form-control', required: true %>
      </div>
    </div>
    <br>
    <div class="row">
      <div class="col-md-6">
        <%= form.label :complemento %>
        <%= form.text_field :complemento, class: 'form-control' %>
      </div>
      <div class="col-md-6">
        <%= form.label :bairro, class: 'required' %>
        <%= form.text_field :bairro, class: 'form-control', required: true %>
      </div>
    </div>
    <br>
    <div class="row">
      <div class="col-md-4">
        <%= form.label :cidade, class: 'required' %>
        <%= form.text_field :cidade, class: 'form-control', required: true %>
      </div>
      <div class="col-md-4">
        <%= form.label :uf, class: 'required' %>
        <%= form.select :uf, InscricaoServidor.uf, { prompt: 'Selecione' }, class: 'form-control' %>
      </div>
      <div class="col-md-4">
        <%= form.label :cep, class: 'required' %>
        <%= form.text_field :cep, class: 'form-control mask-cep', required: true %>
      </div>
    </div>
    <br>
    <div class="row">
      <div class="col-md-6">
        <%= form.label :telefone_fixo %>
        <%= form.telephone_field :telefone_fixo, class: 'form-control mask-phone-fix' %>
      </div>
      <div class="col-md-6">
        <%= form.label :telefone_celular, class: 'required' %>
        <%= form.telephone_field :telefone_celular, class: 'form-control mask-phone-cel', required: true %>
        <small id="valid_telefone_celular" class="text-danger" style="text-transform: none; display: none;">Favor informe o número do celular completo de acordo com o padrão <b>(85) 9.9999-9999</b></small>
      </div>
    </div>
    <br>
    <div class="row">
      <div class="col-md-12">
        <%= form.label :email, class: 'required' %>
        <%= form.email_field :email, class: 'form-control', required: true %>
      </div>
    </div>

    <br>
    <div class="row">
      <div class="col-md-12">
        <%= form.check_box :cota_negro %>
        <label for="inscricao_servidor_cota_negro"><b>Candidato Negro?</b></label>
        <p style="font: italic 13px arial;margin-left: 20px;color: #585858;">
          5.10. Os candidatos negros (pretos ou pardos) concorrerão concomitantemente às vagas reservadas para negros e às vagas destinadas à ampla concorrência, de acordo com a sua classificação na Seleção.
          <br>
          5.8. Será eliminado da Seleção o candidato que:
          a) não for considerado negro pela comissão de heteroidentificação;
          b) se recusar a ser filmado;
          c) prestar declaração falsa;
        </p>
      </div>
    </div>

    <%= form.check_box :deficiente %>
    <% if(!form.object.deficiente_anexo.blank?) %>
      <label for="inscricao_servidor_deficiente" class="required"><b>Pessoa com deficiência? (PDF máximo 5Mb)</b></label>
    <% else %>
      <label for="inscricao_servidor_deficiente"><b>Pessoa com deficiência (PDF máximo 5Mb)</b></label>
    <% end %>
    <div id="inscricao_servidor_deficiente_fl" style="display:none">
      <%= form.file_field :deficiente_anexo, class: 'form-control' %>
      <%= form.hidden_field :deficiente_anexo_cache %>
    </div>
    <% if(!form.object.deficiente_anexo.blank?) %>
      <br><a target="blank" href="<%= form.object.deficiente_anexo %>"><%= form.object.deficiente_anexo.content_type.include?('image') ? image_tag("img.png", size: "28x35") : image_tag("pdf.png", size: "28x35") %></a>
      <a target="blank" href="<%= form.object.deficiente_anexo %>"><%= form.object.deficiente_anexo.identifier %></a>
    <% end %>
    <br>
    <h2>Documentos</h2>

    <div>
      <div class="topic_docs">1</div>
      <hr class="linha_hr">
    </div>
    <br>

    <% if(!form.object.documentos.blank?) %>
      <label class='required'>Anexar Documento Pessoal com foto, conforme item 8.1 do Edital (PDF máximo 5Mb)</label>
      <br><small>Em caso de dois ou mais documentos, escaneie e anexe-os em um único arquivo.</small>
      <%= form.file_field :documentos, class: 'form-control' %><br>
      <%= form.hidden_field :documentos_cache %>
      <a target="blank" href="<%= form.object.documentos %>"><%= form.object.documentos.content_type.include?('image') ? image_tag("img.png", size: "28x35") : image_tag("pdf.png", size: "28x35") %></a>
      <a target="blank" href="<%= form.object.documentos %>"><%= form.object.documentos.identifier %></a><br>
    <% else %>
      <label for="inscricao_servidor_documentos" class="required"><b>Anexar Documento Pessoal com foto, conforme item 8.1 do Edital (PDF máximo 5Mb)</b></label>
      <br><small>Em caso de dois ou mais documentos, escaneie e anexe-os em um único arquivo.</small>
      <%= form.file_field :documentos, class: 'form-control', required: true %><br>
      <%= form.hidden_field :documentos_cache %>
    <% end %>

    <br>
    <div>
      <div class="topic_docs">2</div>
      <hr class="linha_hr">
    </div>
    <br>

    <div id="nivel_superior" class="alert alert-info" role="alert" style="background-color: #dfe4ea;border: 0px solid #000;color: #212529;">
      <div class="col-md-12">
        <b><label>Informar os campos abaixo somente para os cargos de Assistente Social, Psicólogo e Pedagogo</label></b><hr>
        <div class="row">
          <div class="col-md-6">
            <%= form.label :orgao_representacao_classe %>
            <%= form.select :orgao_representacao_classe, ["Conselho Regional de Serviço Social - CRESS", "Conselho Regional de Psicologia - CRP", "Conselho Federal de Educadores e Pedagogos - CFEP"], {prompt: "selecione"}, class: 'form-control' %> <br>
          </div>
          <div class="col-md-6">
            <%= form.label :numero_registro %>
            <%= form.text_field :numero_registro, class: 'form-control' %>
          </div>
        </div>
        <div class="row">
          <div class="col-md-12">
            <label class="required">Anexar documento comprobatório órgão representação de classe (PDF máximo 5Mb)</label>
            <% if(!form.object.represetacao_classe.blank?) %>
              <%= form.file_field :represetacao_classe, class: 'form-control' %><br>
              <%= form.hidden_field :represetacao_classe_cache %>
              <a target="blank" href="<%= form.object.represetacao_classe %>"><%= form.object.represetacao_classe.content_type.include?('image') ? image_tag("img.png", size: "28x35") : image_tag("pdf.png", size: "28x35") %></a>
              <a target="blank" href="<%= form.object.represetacao_classe %>"><%= form.object.represetacao_classe.identifier %></a>
            <% else %>
              <%= form.file_field :represetacao_classe, class: 'form-control', required: true %><br>
              <%= form.hidden_field :represetacao_classe_cache %>
            <% end %>
          </div>
        </div>
        <div class="row">
          <div class="col-md-12">
            <% if(!form.object.certificado_ensino_superior.blank?) %>
              <b>Anexar diploma de conclusão de ensino superior (PDF máximo 5Mb)</b>
              <br><small>Em caso de dois ou mais documentos, escaneie e anexe-os em um único arquivo.</small>
            <% else %>
              <label for="inscricao_servidor_certificado_ensino_superior" class="required"><b>Anexar diploma de conclusão de ensino superior (PDF máximo 5Mb)</b></label>
              <br><small>Em caso de dois ou mais documentos, escaneie e anexe-os em um único arquivo.</small>
            <% end %>
            
            <% if(!form.object.certificado_ensino_superior.blank?) %>
              <%= form.file_field :certificado_ensino_superior, class: 'form-control' %>
              <%= form.hidden_field :certificado_ensino_superior_cache %>
              <br><a target="blank" href="<%= form.object.certificado_ensino_superior %>"><%= form.object.certificado_ensino_superior.content_type.include?('image') ? image_tag("img.png", size: "28x35") : image_tag("pdf.png", size: "28x35") %></a>
              <a target="blank" href="<%= form.object.certificado_ensino_superior %>"><%= form.object.certificado_ensino_superior.identifier %></a>
            <% else %>
              <%= form.file_field :certificado_ensino_superior, class: 'form-control', required: true %>
              <%= form.hidden_field :certificado_ensino_superior_cache %>
            <% end %>
          </div>
        </div>
      </div>
    </div>
    <div id="nivel_ensino_medio" class="alert alert-info" role="alert" style="background-color: #dfe4ea;border: 0px solid #000;color: #212529;">
      <div class="col-md-12">
        <% if(!form.object.certificado_ensino_medio.blank?) %>
          <b>Anexar diploma de conclusão de ensino médio (PDF máximo 5Mb)</b>
          <br><small>Em caso de dois ou mais documentos, escaneie e anexe-os em um único arquivo.</small>
          <%= form.file_field :certificado_ensino_medio, class: 'form-control' %><br>
          <%= form.hidden_field :certificado_ensino_medio_cache %>
          <a target="blank" href="<%= form.object.certificado_ensino_medio %>"><%= form.object.certificado_ensino_medio.content_type.include?('image') ? image_tag("img.png", size: "28x35") : image_tag("pdf.png", size: "28x35") %></a>
          <a target="blank" href="<%= form.object.certificado_ensino_medio %>"><%= form.object.certificado_ensino_medio.identifier %></a>
        <% else%>
          <label for="inscricao_servidor_certificado_ensino_medio" class="required">
            <b>Anexar diploma de conclusão de ensino médio (PDF máximo 5Mb)</b>
            <br><small>Em caso de dois ou mais documentos, escaneie e anexe-os em um único arquivo.</small>
          </label>
          <%= form.file_field :certificado_ensino_medio, class: 'form-control', required: true %>
          <%= form.hidden_field :certificado_ensino_medio_cache %>
        <% end %>
      </div>
    </div>
    <br><br>
    <div id="error_explanation">
      <div class="alert alert-info" role="alert" style="center;color:#2b2b2b;background-color:#fff7db;border-color: #f2dd88;">
        <h1><i class="fas fa-exclamation-triangle" style="color: #2b2b2b;"></i> Atenção</h1>
        <p>Prezado candidato, somente finalize sua inscrição, clicando no botão de cor verde ("Finalizar Inscrição"), localizado no final deste formulário, após realizar a conferência dos seus dados.</p>
      </div>
    </div><br>    
    <h2 style="text-align: center;">Itens de Avaliação</h2>
    <table class="table">
      <thead class="thead-dark">
        <tr>
          <th scope="col" style="width: 33px;color: #1e2124;border-color: #93b46f; background-color: #b2d190;">#</th>
          <th scope="col" style="width: 350px;color: #1e2124;border-color: #93b46f; background-color: #b2d190;">Formação Acadêmica</th>
          <th scope="col" style="width: 175px;color: #1e2124;border-color: #93b46f; background-color: #b2d190;">Pontuação Máxima</th>
          <th scope="col" style="width: 130px;color: #1e2124;border-color: #93b46f; background-color: #b2d190;">Pontuação</th>
          <th scope="col" style="color: #1e2124;border-color: #93b46f; background-color: #b2d190;"><div class="tamanho_mb">Tamanho do PDF 5MB</div></th>
        </tr>
      </thead>   

      <tbody>
        <tr>
          <td>1</td>
          <td>Doutorado (em área correlata a Socioeducação)</td>
          <td><span id="formacao_academica_01"></span></td>
          <td>
            <%= form.select :formacao_academica_01_candidato, ["0.0", "7.0"], {}, class: 'form-control', onchange:"calcular_formacao_academica();validar_pontuacao_formacao_academica(this);",  onblur:"calcular_formacao_academica();validar_pontuacao_formacao_academica(this);", "data-html":"true", "data-toggle":"tooltip", "data-placement":"top", title:"<b>Exemplo:</b><br>1 Curso = 7 pontos" %>
          </td>
          <td>
            <% if(!form.object.formacao_academica_01_file.blank?) %>
              <div id="display_formacao_academica_01" class="display"><div onclick="close_display()" class="close_display"><%= image_tag("close.png", size: "20x20") %></div><%= video_tag "video_anexo.mp4", width: "100%", height:"100%", controls: true, autoplay: true, loop: true %></div>
              <%= form.file_field :formacao_academica_01_file, multiple: true, class: 'form-control file', onchange:"validar_anexo_formacao_academica(this)", tem_anexo:"SIM",  onmouseover:"exibir_display(this)" %>
              <%= link_to image_tag("clear.png").html_safe, remover_file_inscricao_servidors_path(slug: form.object.slug, file: "formacao_academica_01_file"), class: 'file_clear', onclick:"return confirm('Deseja realmente remover o arquivo?');" %>
              <%= form.hidden_field :formacao_academica_01_file_cache %>
              <% form.object.formacao_academica_01_file.each do |file| %>
                <span class="font_anexo">
                  <a target="blank" href="<%= file %>">
                    <%= file.content_type.include?('image') ? image_tag("img.png", size: "15x20") : image_tag("pdf.png", size: "15x20") %> 
                    <%= file.identifier.gsub("_"," ").gsub(" .",".") %>
                  </a><br>
                </span>
              <% end %>
            <% elsif %>
              <div id="display_formacao_academica_01" class="display"><div onclick="close_display()" class="close_display"><%= image_tag("close.png", size: "20x20") %></div><%= video_tag "video_anexo.mp4", width: "100%", height:"100%", controls: true, autoplay: true, loop: true %></div>
              <%= form.file_field :formacao_academica_01_file, multiple: true, class: 'form-control file', onchange:"validar_anexo_formacao_academica(this)", tem_anexo:"NAO",  onmouseover:"exibir_display(this)" %>
              <a id="formacao_academica_01_link" href="#" class="link file_clear" onclick="remover(this)"><%= image_tag("clear.png") %></a>
              <%= form.hidden_field :formacao_academica_01_file_cache %>
            <% end %>
          </td>
        </tr>
        <tr>
          <td>2</td>
          <td>Doutorado (em qualquer área)</td>
          <td><span id="formacao_academica_02"></span></td>
          <td>
            <%= form.select :formacao_academica_02_candidato, ["0.0", "5.0"], {}, class: 'form-control', onchange:"calcular_formacao_academica();validar_pontuacao_formacao_academica(this);",  onblur:"calcular_formacao_academica();validar_pontuacao_formacao_academica(this);", "data-html":"true", "data-toggle":"tooltip", "data-placement":"top", title:"<b>Exemplo:</b><br>1 Curso = 5 pontos" %>
          </td>
          <td>
            <% if(!form.object.formacao_academica_02_file.blank?) %>
              <div id="display_formacao_academica_02" class="display"><div onclick="close_display()" class="close_display"><%= image_tag("close.png", size: "20x20") %></div><%= video_tag "video_anexo.mp4", width: "100%", height:"100%", controls: true, autoplay: true, loop: true %></div>
              <%= form.file_field :formacao_academica_02_file, multiple: true, class: 'form-control file', onchange:"validar_anexo_formacao_academica(this)", tem_anexo:"SIM",  onmouseover:"exibir_display(this)" %>
